<config>
    <moduleName>akonadi</moduleName>
    <parentModules>
        <module>kdecore</module>
        <module>kdeui</module>
    </parentModules>
    <parts>10</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <exclude>
        <!-- private stuff -->
        <signature>.*::d</signature>
        <signature>.*::d_ptr</signature>
        <signature>.*::q_ptr</signature>
        <signature>.*::q</signature>
        <signature>.*QDebug.*</signature>
        <signature>.*Q3.*</signature>
        
        <signature>.*Akonadi::EntityMimeTypeFilterModel::contentMimeTypeInclusionFilters\(\) const</signature>
        <signature>.*Akonadi::EntityMimeTypeFilterModel::addContentMimeTypeInclusionFilters\(const QStringList&amp;\)</signature>
        <signature>.*Akonadi::EntityMimeTypeFilterModel::addContentMimeTypeInclusionFilter\(const QString&amp;\)</signature>
    </exclude>
    <functions>
        <!-- everything in that namespace.. -->
        <name>Akonadi::.*</name>
    </functions>
    <classList>
