<config>
    <moduleName>nepomuk</moduleName>
    <parts>10</parts>
    <parentModules>
        <module>qtcore</module>
        <module>qtgui</module>
        <module>kdecore</module>
        <module>kdeui</module>
        <module>soprano</module>
    </parentModules>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <exclude>
        <!-- private stuff -->
        <signature>.*::d</signature>
        <signature>.*::d_ptr</signature>
        <signature>.*::q_ptr</signature>
        <signature>.*::q</signature>
        <signature>.*QDebug.*</signature>
        <signature>.*Q3.*</signature>
        
        <signature>.*Nepomuk::ResourceManager::allResources\(\).*</signature>
    </exclude>
    <functions>
        <!-- everything in that namespace.. -->
        <name>Nepomuk::.*</name>
    </functions>
    <classList>
        <class>KRatingWidget</class>
        <class>KTagCloudWidget</class>
        <class>KTagDisplayWidget</class>
        <class>Nepomuk</class>
        <class>Nepomuk::Class</class>
        <class>Nepomuk::DesktopOntologyLoader</class>
        <class>Nepomuk::Entity</class>
        <class>Nepomuk::FileOntologyLoader</class>
        <class>Nepomuk::Literal</class>
        <class>Nepomuk::NepomukOntologyLoader</class>
        <class>Nepomuk::Ontology</class>
        <class>Nepomuk::OntologyLoader</class>
        <class>Nepomuk::OntologyManager</class>
        <class>Nepomuk::Property</class>
        <class>Nepomuk::Resource</class>
        <class>Nepomuk::ResourceManager</class>
        <class>Nepomuk::Service</class>
        <class>Nepomuk::Tag</class>
        <class>Nepomuk::TagCloud</class>
        <class>Nepomuk::TagWidget</class>
        <class>Nepomuk::Thing</class>
        <class>Nepomuk::Types</class>
        <class>Nepomuk::Types::Class</class>
        <class>Nepomuk::Types::Entity</class>
        <class>Nepomuk::Types::Literal</class>
        <class>Nepomuk::Types::Ontology</class>
        <class>Nepomuk::Types::Property</class>
        <class>Nepomuk::Variant</class>
    </classList>
</config>
